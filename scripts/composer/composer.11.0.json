{
  "name": "divanteltd/pimcoredevkit",
  "type": "pimcore-bundle",
  "license": "GPL-3.0-or-later",
  "description": "Devkit is a set of command-line tools helping you develop your Pimcore application.",
  "keywords": [
    "pimcore",
    "pimcore-plugin"
  ],
  "homepage": "https://divante.co",
  "authors": [
    {
      "name": "Jakub PÅ‚askonka",
      "email": "jplaskonka@divante.pl",
      "homepage": "https://divante.co",
      "role": "Developer"
    },
    {
      "name": "Bruno Ramalho",
      "email": "bramalho@divante.pl",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/phpunit-bridge": "*",
    "divante-ltd/pimcore-coding-standards": "*",
    "phpmd/phpmd": "*",
    "sebastian/phpcpd": ">=6.0.0",
    "phploc/phploc": "*",
    "pimcore/pimcore": "v11.0.0-ALPHA7",
    "codeception/codeception": ">=4.2.2"
  },
  "autoload": {
    "psr-4": {
      "PimcoreDevkitBundle\\": "src/"
    }
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "PimcoreDevkitBundle\\PimcoreDevkitBundle"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true,
      "ocramius/package-versions": true
    }
  }
}

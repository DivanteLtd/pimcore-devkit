{
  "name": "divanteltd/pimcoredevkit",
  "type": "pimcore-bundle",
  "license": "GPL-3.0-or-later",
  "description": "Devkit is a set of command-line tools helping you develop your Pimcore application.",
  "keywords": [
    "pimcore",
    "pimcore-plugin"
  ],
  "homepage": "https://divante.co",
  "authors": [
    {
      "name": "Jakub PÅ‚askonka",
      "email": "jplaskonka@divante.pl",
      "homepage": "https://divante.co",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1",
    "pimcore/core-version": "^5.1.0"
  },
  "require-dev": {
    "phpstan/phpstan-shim": "^0.9.2",
    "squizlabs/php_codesniffer": "^3.2",
    "friendsofphp/php-cs-fixer": "^2.11"
  },
  "autoload": {
    "psr-4": {
      "PimcoreDevkitBundle\\": "src/PimcoreDevkitBundle"
    }
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "PimcoreDevkitBundle\\PimcoreDevkitBundle"
      ]
    }
  }
}
 
